# Invoice Pipeline Environment Variables - Phase 3
# Copy this file to .env and fill in your values

# === AWS Configuration ===
# S3 bucket name (get from CDK output after deployment)
S3_BUCKET=invoice-pipeline-bucket-xxxxx
S3_REGION=us-east-1
S3_PREFIX=invoices/

# === RDS Configuration ===
# PostgreSQL connection settings
RDS_HOST=localhost
RDS_PORT=5432
RDS_USER=postgres
RDS_PASSWORD=your-secure-password
RDS_DB=invoices

# === Lambda Configuration (set via CDK, not here) ===
# INVOICE_BUCKET=<set by CDK>
# TEXTRACT_ENABLED=true
# TEXTRACT_CONFIDENCE_THRESHOLD=70
# LOG_LEVEL=INFO
